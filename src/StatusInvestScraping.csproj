<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>tbertuzzi</Authors>
    <Company>KBits</Company>
    <Copyright>Copyright ©2021 KBits</Copyright>
    <Version>1.0.0</Version>
    <PackageProjectUrl>https://github.com/TBertuzzi/StatusInvestScraping</PackageProjectUrl>
    <RepositoryUrl>https://github.com/TBertuzzi/StatusInvestScraping</RepositoryUrl>
    <PackageReleaseNotes>Primeira versão para baixar ações e fundos imobiliarios do site StatusInvest</PackageReleaseNotes>
    <Description>Pacote para converter informações de ativos do site StatusInvest em Objetos</Description>
    <PackageIcon>icone.png</PackageIcon>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CsQuery.NETStandard" Version="1.3.6.1" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\Resources\icone.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
